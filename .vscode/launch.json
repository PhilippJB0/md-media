  "version": "0.2.0",
  "configurations": [{
      "name": "Test",
      "type": "node",
      "request": "launch",
      "program": "/usr/local/bin/jest",
      "stopOnEntry": false,
      "args": ["--runInBand", "--forceExit"],
      "cwd": "${workspaceRoot}/",
      "preLaunchTask": null,
      "runtimeExecutable": null,
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "JEST_TIMEOUT": "300000"
      },
      "externalConsole": false,
      "sourceMaps": false,
      "outDir": null
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceRoot}/index.js"
    }
  ]
}